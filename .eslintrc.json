{
	"extends": [
		"next/core-web-vitals",
		"plugin:tailwindcss/recommended",
		"plugin:import/errors",
		"plugin:import/warnings"
	],
	"plugins": ["import"],
	"rules": {
		"import/order": [
			"error",
			{
				"groups": ["builtin", "external", "parent", "sibling", "index"],
				"newlines-between": "always"
			}
		]
	}
}
